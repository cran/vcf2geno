CC=@CC@
CXX=g++
PKG_CFLAGS=-I. -DNDEBUG -O3 -pipe -g
PKG_CXXFLAGS=-I. -DNDEBUG  -O3 -pipe -g
PKG_LIBS=-lz -lm -lbz2

